package thread2;

import java.util.concurrent.locks.ReentrantLock;

public class MyThread2 extends Thread{

	ReentrantLock rl = new ReentrantLock();
	public void run() {
		if(rl.tryLock()) {
			System.out.println("first thread get the lock");
			Thread.sleep(2000);
		}else {
			System.out.println();
		}
	}
}
